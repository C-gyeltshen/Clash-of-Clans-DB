generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model player_login {
  login_id     Int       @id @default(autoincrement())
  player_id    Int?
  login_time   DateTime? @db.Time(6)
  device_info  String?   @db.VarChar(100)
  ip_address   String?   @db.Inet
  login_status String?   @db.VarChar(100)
}
